<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="276.0" prefWidth="502.0" spacing="16.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ServerSocket">
   <children>
      <AnchorPane styleClass="underline">
         <children>
            <Label fx:id="titleLabel" prefHeight="25.0" prefWidth="438.0" styleClass="title-3" text="BLib Server Socket" textAlignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <VBox spacing="8.0">
         <children>
            <Label text="Port" />
            <TextField fx:id="Porttxt" prefHeight="25.0" prefWidth="438.0" text="5555" />
         </children>
      </VBox>
      <Label fx:id="portWarning" styleClass="danger" text="You must enter port!" visible="false" />
      <Button id="btnCreate" mnemonicParsing="false" onAction="#Create" prefHeight="36.0" prefWidth="461.0" text="Start" />
   </children>
   <padding>
      <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
   </padding>
</VBox>

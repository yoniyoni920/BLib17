<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="311.0" prefWidth="502.0" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ClientSocket">
   <children>
      <Label alignment="CENTER" prefHeight="25.0" prefWidth="438.0" text="BLib Client Socket" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <VBox prefHeight="0.0" prefWidth="438.0">
         <children>
            <Label text="IP" />
            <TextField fx:id="IPtxt" prefHeight="30.0" prefWidth="438.0" text="127.0.0.1">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label fx:id="errorIp" styleClass="danger" text="You must enter an IP" textFill="#fb0000" visible="false" />
         </children>
      </VBox>
      <VBox prefHeight="17.0" prefWidth="438.0">
         <children>
            <Label text="Port" />
            <TextField fx:id="Porttxt" prefHeight="25.0" prefWidth="438.0" text="5555" />
            <Label fx:id="errorPort" styleClass="danger" text="You must enter a port" textFill="#fb0000" visible="false" />
         </children>
      </VBox>
      <Button id="btnConnect" mnemonicParsing="false" onAction="#Connect" prefHeight="36.0" prefWidth="438.0" text="Connect" />
   </children>
   <padding>
      <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
   </padding>
</VBox>

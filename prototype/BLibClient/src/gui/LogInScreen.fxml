<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="TOP_RIGHT" prefWidth="502.0" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LogInScreen">
   <children>
      <Label alignment="CENTER" prefHeight="25.0" prefWidth="438.0" text="BLib Login" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <VBox prefHeight="0.0" prefWidth="438.0">
         <children>
            <Label text="Subscriber ID" />
            <TextField fx:id="idtxt" prefHeight="42.0" prefWidth="431.0">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <Label fx:id="errorLabel" styleClass="danger" text="Label" visible="false" />
         </children>
      </VBox>
      <Button id="btnStudentInfo" fx:id="btnSend" mnemonicParsing="false" onAction="#Login" prefHeight="36.0" prefWidth="463.0" text="Login" />
   </children>
   <padding>
      <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
   </padding>
</VBox>

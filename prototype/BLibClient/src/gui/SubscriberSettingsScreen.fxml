<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="258.0" prefWidth="499.0" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SubscriberSettingsScreen">
   <children>
      <Label alignment="CENTER" prefHeight="32.0" prefWidth="511.0" text="Subscriber Settings" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Phone Number" />
            <TextField fx:id="phoneTxtField" prefHeight="32.0" prefWidth="445.0">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Email" />
            <TextField fx:id="emailTxtField" prefWidth="445.0">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <HBox alignment="TOP_RIGHT" prefHeight="36.0" prefWidth="134.0" spacing="4.0">
         <children>
            <Button mnemonicParsing="false" onAction="#backBtn" text="Back" />
            <Button mnemonicParsing="false" onAction="#updateInfo" text="Update" />
         </children>
         <VBox.margin>
            <Insets top="16.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
   </padding>
</VBox>
